{
  "expo": {
    "name": "Bitirme Face",
    "slug": "bitirme-face",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Kamera yuz kaydi ve tanima icin kullanilir.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    }
  }
}